"LightmappedGeneric"
{
	"$baseTexture" "Wall/HR_Wall_Glass01"
	"$surfaceprop" "glass"
	"$translucent" 1
	"$envmap" "env_cubemap"
	"$envmaptint" "[.95 .95 .95]"
	"$nocull" 1
	"$crackmaterial" "glass/glasswindowbreak070b"
}

"LightmappedGeneric_dx8"
{
	"$envmaptint" "[.95 .95 .95]"
}

"LightmappedGeneric_dx6"
{
